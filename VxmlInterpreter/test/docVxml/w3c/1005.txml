<vxml version="2.0" xmlns="http://www.w3.org/2001/vxml" xmlns:conf="http://www.w3.org/2002/vxml-conformance">
    <catch><conf:fail expr="'catch-all caught ' + _event"/></catch>
    <form>
        <block>
            <goto nextitem="pass"/>
        </block>
        <block name="bad1">
            <conf:fail expr="'executed form item that should have been skipped'"/>
        </block>
        <block name="pass">
            <conf:pass/>
        </block>
        <block name="bad3">
            <conf:fail expr="'executed form item that should next be reached'"/>
        </block>
    </form>
</vxml>
